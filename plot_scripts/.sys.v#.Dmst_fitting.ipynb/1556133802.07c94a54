    "    if(useRooFit):\n",
    "        mean = rt.RooRealVar('mean', 'm_{D0}', m_Dst, 0, 5, 'GeV')\n",
    "        sigma = rt.RooRealVar('sigma', '#sigma', 0.001, 0.00005, 0.05, 'GeV') \n",
    "        pdf_sig = rt.RooGaussian('gaus','sig pdf', m, mean, sigma)\n",
    "        \n",
    "        nsig = rt.RooRealVar('nsig', 'N_{S}', 3000, 0, 1e6)\n",
