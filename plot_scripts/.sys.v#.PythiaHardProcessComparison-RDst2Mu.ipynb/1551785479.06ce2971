    "    SetMaxToMaxHist(h_l)\n",
    "#     c_list.append(drawOnCMSCanvas(h_l+[leg], 'same', v))\n",
    "    c = make_ratio_plot(h_l, label=v)\n",
    "    c.Draw()\n",
    "    c_list.append(c)"
   ]
