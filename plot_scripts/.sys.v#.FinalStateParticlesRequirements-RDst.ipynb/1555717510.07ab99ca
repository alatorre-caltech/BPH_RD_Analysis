   "source": [
    "def drawOnCMSCanvas(hl, opt = None, tag=''):\n",
    "    c = rt.TCanvas('c'+tag, 'c'+tag, 50, 50, 800, 600)\n",
    "    c.SetTickx(0)\n",
    "    c.SetTicky(0)\n",
    "    \n",
    "    for i, o in enumerate(hl):\n",
    "        do = ''\n",
    "        if not opt is None:\n",
    "            if opt == 'same' and i>0:\n",
    "                do = 'SAME'\n",
    "            else:\n",
    "                do = opt[i]\n",
    "        o.Draw(do)\n",
    "        \n",
    "    \n",
    "    CMS_lumi.CMS_lumi(c, -1, 0)\n",
    "    c.objs = [hl]\n",
    "    c.Draw()\n",
    "    return c"
   ]
