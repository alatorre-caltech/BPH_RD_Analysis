   ],
   "source": [
    "t = tree[n]\n",
    "muRECO_sel  = np.logical_and(np.abs(t['mu_RecoMC_dpt']) < 0.15 , tree[n]['mu_RecoMC_dR'] < 0.03)\n",
    "\n",
    "dpt = np.abs(1 - t['muReco_pt']/t['trgMu_pt'])\n",
    "dphi = t['muReco_phi']-t['trgMu_phi']\n",
    "dphi = np.where(np.abs(dphi)>np.pi, dphi - np.sign(dphi)*2*np.pi, dphi)\n",
    "dR = np.hypot(dphi, t['muReco_eta']-t['trgMu_eta'])\n",
    "c = dr_dpt_plot(dpt[muRECO_sel] , dR[muRECO_sel], [0.01, 0.005])\n",
    "\n",
    "muB_trg_sel = np.logical_and(dpt < 0.01, dR < 0.005)"
   ]
  },
