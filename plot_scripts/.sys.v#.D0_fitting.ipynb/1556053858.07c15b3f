    "def extarct(t, branches = [], N = None):\n",
    "    if len(branches) == 0:\n",
    "        branches = t.keys()\n",
    "    l = {}\n",
    "    for k in branches:\n",
    "        print 'Loading branch', k\n",
    "        m = []\n",
    "        for i, e in enumerate(t.array(k)):\n",
    "            m += list(e)\n",
    "        l[k] = np.array(m)\n",
    "    \n",
    "    return l"
   ]
